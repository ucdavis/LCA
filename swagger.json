{
  "swagger": "2.0",
  "info": {
    "description": "This is a service for running LCA, funded by California Energy Commission",
    "version": "1.6.1",
    "title": "Life Cycle Assessment",
    "contact": {
      "email": "kkyli@ucdavis.edu"
    },
    "license": {
      "name": "MIT license",
      "url": "https://github.com/ucdavis/LCA/blob/master/LICENSE"
    }
  },
  "paths": {
    "/lca": {
      "get": {
        "tags": ["Life cycle emissions accounting"],
        "summary": "Compute life cycle emissions and impacts",
        "description": "",
        "operationId": "runLCA",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "technology",
            "description": "Power Generation Technology",
            "in": "query",
            "required": true,
            "type": "string",
            "enum": ["GPO", "CHP", "GP"],
            "default": "CHP"
          },
          {
            "name": "diesel",
            "description": "Diesel consumption (gal/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 5
          },
          {
            "name": "gasoline",
            "description": "Gasoline consumption (gal/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 3
          },
          {
            "name": "jetfuel",
            "description": "Jet fuel consumption (gal/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 7
          },
          {
            "name": "distance",
            "description": "The distance between a harvest unit and a biopower facility (miles/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 20
          },
          {
            "name": "construction",
            "description": "The capital cost for constructing a biopower facility, in 2002 dollars",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 0
          },
          {
            "name": "equipment",
            "description": "The purchase price of a piece of equipment before tax, in 2002 dollars",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 0
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/LcaOutputs"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    }
  },
  "definitions": {
    "LcaOutputs": {
      "type": "object",
      "properties": {
        "lifeCycleEmissions": {
          "$ref": "#/definitions/LifeCycleEmissions"
        },
        "lifeCycleImpacts": {
          "$ref": "#/definitions/LifeCycleImpacts"
        }
      },
      "xml": {
        "name": "LcaOutputs"
      }
    },
    "LifeCycleEmissions": {
      "type": "object",
      "properties": {
        "CO2": {
          "type": "number",
          "format": "float",
          "description": "Carbon dioxide emissions (kg)",
          "example": 216.52
        },
        "CH4": {
          "type": "number",
          "format": "float",
          "description": "Methane emissions (g)",
          "example": 12.53
        },
        "N2O": {
          "type": "number",
          "format": "float",
          "description": "Nitrous oxide emissions (g)",
          "example": 2.22
        },
        "CO": {
          "type": "number",
          "format": "float",
          "description": "Carbon monoxide emissions (g)",
          "example": 8802.96
        },
        "NOx": {
          "type": "number",
          "format": "float",
          "description": "Nitrogen oxides emissions (g)",
          "example": 572.42
        },
        "PM10": {
          "type": "number",
          "format": "float",
          "description": "Particulate matter 10 micrometers or less in diameter (g)",
          "example": 102.06
        },
        "PM2.5": {
          "type": "number",
          "format": "float",
          "description": "Particulate matter 2.5 micrometers or less in diameter (g)",
          "example": 77.84
        },
        "SOx": {
          "type": "number",
          "format": "float",
          "description": "Sulfur oxides emissions (g)",
          "example": 34.06
        },
        "VOC": {
          "type": "number",
          "format": "float",
          "description": "Volatile organic compounds (g)",
          "example": 176.14
        },
        "CI": {
          "type": "number",
          "format": "float",
          "description": "Carbon intensity (kg CO2e)",
          "example": 231.85
        }
      },
      "xml": {
        "name": "LciResults"
      }
    },
    "LifeCycleImpacts": {
      "type": "object",
      "properties": {
        "global_warming_air": {
          "type": "number",
          "format": "float",
          "description": "Global Warming Potentials for Air Emissions (kg CO2 eq)",
          "example": 217.46
        },
        "acidification_air": {
          "type": "number",
          "format": "float",
          "description": "Acidification Potentials for Air Emissions (kg SO2 eq)",
          "example": 0.43
        },
        "hh_particulate_air": {
          "type": "number",
          "format": "float",
          "description": "Human Health Particulate (and secondary particulate matter precursors) Potentials for Air Emissions (PM2.5eq)",
          "example": 0.03
        },
        "eutrophication_air": {
          "type": "number",
          "format": "float",
          "description": "Eutrophication Potentials for Air Emissions (kg N eq)",
          "example": 0.03
        },
        "smog_air": {
          "type": "number",
          "format": "float",
          "description": "Smog Formation Potentials for Air Emissions (kg O3 eq)",
          "example": 14.68
        }
      },
      "xml": {
        "name": "LciaResults"
      }
    }
  },
  "externalDocs": {
    "description": "Source code",
    "url": "https://github.com/ucdavis/LCA"
  }
}
